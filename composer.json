{
  "type": "project",
  "name": "tacman/member-directory",
  "description": "Fork of https://github.com/utmsigep/member-directory",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.3",
    "ext-apcu": "*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "^4.0",
    "beberlei/doctrineextensions": "^1.2",
    "bizkit/versioning-bundle": "^1.2",
    "campaignmonitor/createsend-php": "^7.0",
    "cmen/google-charts-bundle": "^7.0",
    "composer/package-versions-deprecated": "^1.11",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-fixtures-bundle": "^3.5 || ^4.0",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.10|^3.2",
    "endroid/qr-code": "^6.0.3",
    "erusev/parsedown": "2.0.x-dev",
    "friendsofsymfony/jsrouting-bundle": "^3.3",
    "gedmo/doctrine-extensions": "^3.6",
    "genyaa/usps-php-api": "^2.0.3",
    "geocodio/geocodio-library-php": "^2.0",
    "league/commonmark": "^2.6",
    "league/csv": "^9.2",
    "meilisearch/meilisearch-php": "^1.7",
    "nelmio/cors-bundle": "*",
    "nyholm/psr7": "^1.0",
    "sabre/vobject": "^4.4",
    "scheb/2fa-bundle": "^7.1",
    "scheb/2fa-totp": "^7.1",
    "scheb/2fa-trusted-device": "^7.1",
    "sebastian/version": "^6.0",
    "sentry/sentry-symfony": "^5.0",
    "stof/doctrine-extensions-bundle": "^1.7",
    "survos/api-grid-bundle": "^1.5",
    "survos/auth-bundle": "^1.5",
    "survos/bootstrap-bundle": "^1.5",
    "survos/command-bundle": "*",
    "survos/crawler-bundle": "^1.5",
    "survos/inspection-bundle": "^1.5",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^7.1",
    "symfony/asset-mapper": "^7.0",
    "symfony/cache": "^7.1",
    "symfony/console": "^7.1",
    "symfony/dotenv": "^7.1",
    "symfony/expression-language": "^7.1",
    "symfony/flex": "^2.3",
    "symfony/form": "^7.1",
    "symfony/framework-bundle": "^7.1",
    "symfony/html-sanitizer": "^7.1",
    "symfony/http-client": "^7.0",
    "symfony/intl": "^7.1",
    "symfony/mailer": "^7.1",
    "symfony/monolog-bundle": "*",
    "symfony/notifier": "^7.1",
    "symfony/panther": "^2.1",
    "symfony/property-access": "^7.1",
    "symfony/property-info": "^7.1",
    "symfony/runtime": "^7.1",
    "symfony/security-bundle": "^7.1",
    "symfony/serializer": "^7.1",
    "symfony/stimulus-bundle": "^2.21",
    "symfony/translation": "^7.1",
    "symfony/twig-bundle": "^7.1",
    "symfony/twilio-notifier": "^7.1",
    "symfony/ux-icons": "^2.21",
    "symfony/ux-leaflet-map": "^2.21",
    "symfony/ux-map": "^2.21",
    "symfony/validator": "^7.1",
    "symfony/yaml": "^7.1",
    "symfonycasts/reset-password-bundle": "^1.17",
    "tacman/dictionary-bundle": "^4.0.6",
    "twig/cssinliner-extra": "^3.6",
    "twig/extra-bundle": "^3.6",
    "twig/inky-extra": "^3.6",
    "twig/intl-extra": "^3.6",
    "twig/markdown-extra": "^3.6",
    "twig/string-extra": "^3.6",
    "twilio/sdk": "^8.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "endroid/installer": true,
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "survos/installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd",
      "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default  \"": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html coverage"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^7.0"
    }
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpunit/phpunit": "^12.0",
    "rector/rector": "^2.0",
    "survos/deployment-bundle": "^1.5",
    "survos/maker-bundle": "^1.5",
    "symfony/browser-kit": "^7.1",
    "symfony/css-selector": "^7.1",
    "symfony/maker-bundle": "^1.49",
    "symfony/phpunit-bridge": "^7.1",
    "symfony/stopwatch": "^7.1",
    "symfony/var-dumper": "^7.1",
    "symfony/web-profiler-bundle": "^7.1",
    "zenstruck/browser": "^1.9",
    "zenstruck/foundry": "^2.0"
  }
}
