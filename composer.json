{
  "type": "project",
  "license": "MIT",
  "minimum-stability": "rc",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-apcu": "*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "^3.2.6",
    "beberlei/doctrineextensions": "^1.2",
    "campaignmonitor/createsend-php": "^7.0",
    "cmen/google-charts-bundle": "^6.0",
    "composer/package-versions-deprecated": "^1.11",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-fixtures-bundle": "^3.5 || ^4.0",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.10|^3.2",
    "endroid/qr-code": "^4.5",
    "erusev/parsedown": "^1.7",
    "friendsofsymfony/jsrouting-bundle": "^3.3",
    "gedmo/doctrine-extensions": "^3.6",
    "genyaa/usps-php-api": "^1.1",
    "geocodio/geocodio-library-php": "^1.4",
    "league/csv": "^9.2",
    "meilisearch/meilisearch-php": "^1.7",
    "nelmio/cors-bundle": "2.x-dev",
    "nyholm/psr7": "^1.0",
    "phpstan/phpdoc-parser": "^1.24",
    "sabre/vobject": "^4.4",
    "scheb/2fa-bundle": "^6.4 || ^7.0",
    "scheb/2fa-totp": "^6.4 || ^7.0",
    "scheb/2fa-trusted-device": "^6.4 || ^7.0",
    "sebastian/version": "^3.0",
    "sentry/sentry-symfony": "^5.0",
    "stof/doctrine-extensions-bundle": "^1.7",
    "survos/api-grid-bundle": "^1.5",
    "survos/auth-bundle": "^1.5",
    "survos/bootstrap-bundle": "^1.5",
    "survos/command-bundle": "^1.5",
    "survos/crawler-bundle": "^1.5",
    "survos/inspection-bundle": "^1.5",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^6.4 || ^7.0",
    "symfony/asset-mapper": "^7.0",
    "symfony/cache": "^6.4 || ^7.0",
    "symfony/console": "^6.4 || ^7.0",
    "symfony/dotenv": "^6.4 || ^7.0",
    "symfony/expression-language": "^6.4 || ^7.0",
    "symfony/flex": "^2.3",
    "symfony/form": "^6.4 || ^7.0",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "symfony/html-sanitizer": "^6.4",
    "symfony/http-client": "^7.0",
    "symfony/intl": "^6.4 || ^7.0",
    "symfony/mailer": "^6.4 || ^7.0",
    "symfony/monolog-bundle": "3.x-dev",
    "symfony/notifier": "^6.4 || ^7.0",
    "symfony/panther": "^2.1",
    "symfony/property-access": "^6.4",
    "symfony/property-info": "^6.4",
    "symfony/proxy-manager-bridge": "^6.4 || ^7.0",
    "symfony/runtime": "^6.4 || ^7.0",
    "symfony/security-bundle": "^6.4 || ^7.0",
    "symfony/serializer": "v6.4.0-RC1 || v7.0.0-RC1",
    "symfony/stimulus-bundle": "^2.8-dev",
    "symfony/templating": "^6.4 || ^7.0",
    "symfony/translation": "^6.4 || ^7.0",
    "symfony/twig-bundle": "^6.4 || ^7.0",
    "symfony/twilio-notifier": "^6.4 || ^7.0",
    "symfony/validator": "^6.4 || ^7.0",
    "symfony/yaml": "^6.4 || ^7.0",
    "symfonycasts/reset-password-bundle": "^1.17",
    "twig/cssinliner-extra": "^3.6",
    "twig/extra-bundle": "^3.6",
    "twig/inky-extra": "^3.6",
    "twig/intl-extra": "^3.6",
    "twig/markdown-extra": "^3.6",
    "twig/string-extra": "^3.6",
    "twilio/sdk": "^6.21"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "phpstan/extension-installer": true,
      "symfony/runtime": true,
      "endroid/installer": true,
      "php-http/discovery": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd",
      "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default  \"": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^7.0"
    }
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.3",
    "phpstan/phpstan-doctrine": "^1.0",
    "phpstan/phpstan-symfony": "^1.0",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.14.5",
    "survos/deployment-bundle": "^1.5",
    "survos/maker-bundle": "^1.5",
    "symfony/browser-kit": "^6.4 || ^7.0",
    "symfony/css-selector": "^6.4 || ^7.0",
    "symfony/maker-bundle": "^1.49",
    "symfony/phpunit-bridge": "^6.4 || ^7.0",
    "symfony/stopwatch": "^6.4 || ^7.0",
    "symfony/var-dumper": "^6.4 || ^7.0",
    "symfony/web-profiler-bundle": "^6.4 || ^7.0",
    "zenstruck/foundry": "^1.36"
  },
  "repositories": {
  }
}
