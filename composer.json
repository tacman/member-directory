{
    "type": "project",
    "license": "MIT",
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.2",
        "campaignmonitor/createsend-php": "^7.0",
        "cmen/google-charts-bundle": "^5.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "erusev/parsedown": "^1.7",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "gedmo/doctrine-extensions": "^3.6",
        "genyaa/usps-php-api": "^1.1",
        "geocodio/geocodio-library-php": "^1.4",
        "league/csv": "^9.2",
        "sabre/vobject": "^4.4",
        "scheb/2fa-bundle": "^5.13",
        "scheb/2fa-qr-code": "^5.13",
        "scheb/2fa-totp": "^5.13",
        "scheb/2fa-trusted-device": "^5.13",
        "sebastian/version": "^3.0",
        "sensio/framework-extra-bundle": "^6.2",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/expression-language": "^5.4",
        "symfony/flex": "^1.1",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/intl": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/monolog-bundle": "^3.3",
        "symfony/notifier": "^5.4",
        "symfony/panther": "^1.0",
        "symfony/proxy-manager-bridge": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/templating": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/twilio-notifier": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^5.4",
        "symfonycasts/reset-password-bundle": "^1.8",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^3.0",
        "twig/inky-extra": "^3.3",
        "twig/intl-extra": "^3.0",
        "twig/markdown-extra": "^3.0",
        "twig/string-extra": "^3.3",
        "twilio/sdk": "^6.21"
    },
    "config": {
        "platform": {
          "php": "7.4.13"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^5.4",
        "symfony/var-dumper": "^5.4",
        "symfony/web-profiler-bundle": "^5.4"
    }
}
